<script>
document.addEventListener('DOMContentLoaded', function() {
    // Handle location selection dropdown
    const locationItems = document.querySelectorAll('.option-list-destination li[data-location-id]');
    const locationIdInput = document.getElementById('selectedLocationId');
    const destinationInput = document.querySelector('.destination-dropdown input[type="text"]');
    const destinationDisplay = document.querySelector('.destination-dropdown .destination h6');
    const destinationSpan = document.querySelector('.destination-dropdown .destination span');

    locationItems.forEach(function(item) {
        item.addEventListener('click', function(e) {
            e.preventDefault();
            
            const locationId = this.getAttribute('data-location-id');
            const locationName = this.getAttribute('data-location-name');
            
            // Update hidden input
            locationIdInput.value = locationId;
            
            // Update display
            destinationInput.value = locationName;
            destinationDisplay.textContent = locationName;
            destinationSpan.textContent = 'Selected Location';
            
            // Close dropdown
            const dropdown = document.querySelector('.custom-select-wrap.three');
            if (dropdown) {
                dropdown.style.display = 'none';
            }
        });
    });

    // Handle Hotel page location selection too
    const hotelLocationItems = document.querySelectorAll('.option-list-destination li[data-location-id]');
    const hotelLocationIdInput = document.querySelector('input[name="locationId"]');
    
    if (hotelLocationItems.length > 0 && hotelLocationIdInput) {
        hotelLocationItems.forEach(function(item) {
            item.addEventListener('click', function(e) {
                e.preventDefault();
                
                const locationId = this.getAttribute('data-location-id');
                const locationName = this.getAttribute('data-location-name');
                
                // Update hidden input
                hotelLocationIdInput.value = locationId;
                
                // Update display
                const hotelDestinationInput = document.querySelector('.destination-dropdown input[type="text"]');
                const hotelDestinationDisplay = document.querySelector('.destination-dropdown .destination h6');
                const hotelDestinationSpan = document.querySelector('.destination-dropdown .destination span');
                
                if (hotelDestinationInput) hotelDestinationInput.value = locationName;
                if (hotelDestinationDisplay) hotelDestinationDisplay.textContent = locationName;
                if (hotelDestinationSpan) hotelDestinationSpan.textContent = 'Selected Location';
                
                // Close dropdown
                const dropdown = document.querySelector('.custom-select-wrap.three');
                if (dropdown) {
                    dropdown.style.display = 'none';
                }
            });
        });
    }
    
    // Update room count hidden input whenever rooms change
    function updateRoomsHiddenInput() {
        const roomCount = document.querySelectorAll('.single-room').length || 1;
        const roomsInput = document.getElementById('roomsInput');
        if (roomsInput) roomsInput.value = roomCount;
    }
    
    // Listen for room add/remove to keep Rooms input up to date
    document.addEventListener('click', function(e) {
        if (e.target.matches('.add-btn') || e.target.closest('.add-btn') ||
            e.target.matches('.room-delete-btn') || e.target.closest('.room-delete-btn')) {
            setTimeout(updateRoomsHiddenInput, 100);
        }
    });
    
    // Form submit: sum all per-room Adults/Children into single total values
    const homeForm = document.querySelector('form.filter-input');
    if (homeForm) {
        homeForm.addEventListener('submit', function() {
            let totalAdults = 0;
            let totalChildren = 0;
            const adultsInputs = homeForm.querySelectorAll('input[name="Adults"]');
            const childrenInputs = homeForm.querySelectorAll('input[name="Children"]');

            adultsInputs.forEach(function(inp) {
                totalAdults += parseInt(inp.value, 10) || 0;
                inp.disabled = true;
            });
            childrenInputs.forEach(function(inp) {
                totalChildren += parseInt(inp.value, 10) || 0;
                inp.disabled = true;
            });

            const aInput = document.createElement('input');
            aInput.type = 'hidden'; aInput.name = 'Adults'; aInput.value = totalAdults;
            homeForm.appendChild(aInput);

            const cInput = document.createElement('input');
            cInput.type = 'hidden'; cInput.name = 'Children'; cInput.value = totalChildren;
            homeForm.appendChild(cInput);
            
            updateRoomsHiddenInput();
        });
    }
    
    updateRoomsHiddenInput();
});
</script>

