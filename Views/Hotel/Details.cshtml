@section Scripts {
    <script>
        $(document).ready(function() {
            // Handle Check Availability button on sidebar
            $('#scroll-btn').on('click', function(e) {
                e.preventDefault();
                
                // Find the currently expanded accordion (selected room)
                var $expandedAccordion = $('.accordion-collapse.show');
                
                if ($expandedAccordion.length > 0) {
                    // A room is selected, find and click the Book Now button in that accordion
                    var $bookNowBtn = $expandedAccordion.find('.check-room-availability-btn');
                    
                    if ($bookNowBtn.length > 0) {
                        // Trigger the Book Now button click
                        $bookNowBtn.click();
                    } else {
              showToast('Please select a room to check availability.', 'error');
                    }
                } else {
                    // No room is selected
                    showToast('Please select a room first.', 'error');
                }
            });
            
            // Handle Book Now button clicks for room availability check
            $('.check-room-availability-btn').on('click', function(e) {
                e.preventDefault();
                
                var $btn = $(this);
                var hotelId = $btn.data('hotel-id');
                var roomId = $btn.data('room-id');
                var roomName = $btn.data('room-name');
                var adults = $btn.data('adults') || 1;
                var children = $btn.data('children') || 0;
                var rooms = $btn.data('rooms') || 1;
                
                // Get dates from data attributes or calendar inputs
                var checkInDate = getCheckInDate($btn);
                var checkOutDate = getCheckOutDate($btn);
                
                // Validate dates
                if (!checkInDate || !checkOutDate) {
                    showToast('Please select check-in and check-out dates.', 'error');
                    return;
                }
                
                var originalHtml = $btn.html();
                $btn.prop('disabled', true).html('<span>Checking Availability...</span>');
                
                // Call our own server endpoint to avoid CORS issues
                var proxyUrl = '/Hotel/CheckRoomAvailability';
                var params = new URLSearchParams({
                    id: hotelId,
                    roomId: roomId,
                    startDate: checkInDate,
                    endDate: checkOutDate
                });
                
                var fullUrl = proxyUrl + '?' + params.toString();
                
                // Use fetch API to call our server endpoint
                fetch(fullUrl)
                    .then(function(response) {
                        if (!response.ok) {
                            throw new Error('API returned status: ' + response.status);
                        }
                        
                        return response.json();
                    })
                    .then(function(data) {
                        $btn.prop('disabled', false).html(originalHtml);
                        
                        if (data && data.length > 0) {
                            var availability = data[0];
                            
                            if (availability.availableRooms > 0) {
                                // Room is available, redirect to checkout
                                var checkoutUrl = '/Checkout/Index?' +
                                    'hotelId=' + encodeURIComponent(hotelId) +
                                    '&roomTypeId=' + encodeURIComponent(roomId) +
                                    '&checkInDate=' + encodeURIComponent(checkInDate) +
                                    '&checkOutDate=' + encodeURIComponent(checkOutDate) +
                                    '&adults=' + encodeURIComponent(adults) +
                                    '&children=' + encodeURIComponent(children) +
                                    '&rooms=' + encodeURIComponent(rooms);
                                
                                window.location.href = checkoutUrl;
                            } else {
                                // Room is not available
                                showToast('Sorry, the ' + roomName + ' room is not available for the selected dates. Please choose different dates or another room type.', 'error');
                            }
                        } else {
                            showToast('Unable to check availability. Please try again.', 'error');
                        }
                    })
                    .catch(function(error) {
                        $btn.prop('disabled', false).html(originalHtml);
                        showToast('Unable to check room availability. Please try again later.', 'error');
                    });
            });
            
            function getCheckInDate($btn) {
                // First try to get from data attribute
                var dataCheckIn = $btn.data('checkin');
                if (dataCheckIn && dataCheckIn.trim() !== '') {
                    return formatDateToAPI(dataCheckIn);
                }
                
                // Try to get from calendar input
                var checkInInput = $('#hotel-details-checkin').val();
                if (checkInInput && checkInInput.trim() !== '') {
                    return formatDateToAPI(checkInInput);
                }
                
                // Return default date from calendar or current date
                return getDefaultCheckInDate();
            }
            
            function getCheckOutDate($btn) {
                // First try to get from data attribute
                var dataCheckOut = $btn.data('checkout');
                if (dataCheckOut && dataCheckOut.trim() !== '') {
                    return formatDateToAPI(dataCheckOut);
                }
                
                // Try to get from calendar input
                var checkOutInput = $('#hotel-details-checkout').val();
                if (checkOutInput && checkOutInput.trim() !== '') {
                    return formatDateToAPI(checkOutInput);
                }
                
                // Return default date from calendar or current date + 3 days
                return getDefaultCheckOutDate();
            }
            
            function getDefaultCheckInDate() {
                // Try to get from calendar default value
                var calendarInput = $('#hotel-details-checkin').val();
                if (calendarInput && calendarInput.includes('-')) {
                    var dates = calendarInput.split('-');
                    return formatDateToAPI(dates[0].trim());
                }
                
                // Use current date
                var today = new Date();
                return formatDateObject(today);
            }
            
            function getDefaultCheckOutDate() {
                // Try to get from calendar default value
                var calendarInput = $('#hotel-details-checkout').val();
                if (calendarInput && calendarInput.includes('-')) {
                    var dates = calendarInput.split('-');
                    var dateStr = dates.length > 1 ? dates[1].trim() : dates[0].trim();
                    return formatDateToAPI(dateStr);
                }
                
                // Use current date + 3 days
                var checkout = new Date();
                checkout.setDate(checkout.getDate() + 3);
                return formatDateObject(checkout);
            }
            
            function formatDateObject(dateObj) {
                var month = (dateObj.getMonth() + 1).toString().padStart(2, '0');
                var day = dateObj.getDate().toString().padStart(2, '0');
                var year = dateObj.getFullYear();
                return month + '/' + day + '/' + year;
            }
            
            function formatDateToAPI(dateStr) {
                if (!dateStr) return null;
                
                // If already in MM/DD/YYYY format
                if (/^\d{2}\/\d{2}\/\d{4}$/.test(dateStr)) {
                    return dateStr;
                }
                
                // If in M/D/YYYY format, pad with zeros
                if (/^\d{1,2}\/\d{1,2}\/\d{4}$/.test(dateStr)) {
                    var parts = dateStr.split('/');
                    return parts[0].padStart(2, '0') + '/' + parts[1].padStart(2, '0') + '/' + parts[2];
                }
                
                // Try parsing text date like "June 16"
                var months = {
                    'january': '01', 'february': '02', 'march': '03', 'april': '04',
                    'may': '05', 'june': '06', 'july': '07', 'august': '08',
                    'september': '09', 'october': '10', 'november': '11', 'december': '12',
                    'jan': '01', 'feb': '02', 'mar': '03', 'apr': '04',
                    'jun': '06', 'jul': '07', 'aug': '08', 'sep': '09',
                    'oct': '10', 'nov': '11', 'dec': '12'
                };
                
                var lowerStr = dateStr.toLowerCase().trim();
                var parts = lowerStr.split(/[\s,]+/);
                var month = '';
                var day = '';
                var year = new Date().getFullYear().toString();
                
                for (var i = 0; i < parts.length; i++) {
                    if (months[parts[i]]) {
                        month = months[parts[i]];
                    } else if (!isNaN(parts[i]) && parts[i].length <= 2) {
                        day = parts[i].padStart(2, '0');
                    } else if (!isNaN(parts[i]) && parts[i].length === 4) {
                        year = parts[i];
                    }
                }
                
                if (month && day) {
                    return month + '/' + day + '/' + year;
                }
                
                // Try parsing as ISO date or other format
                try {
                    var date = new Date(dateStr);
                    if (!isNaN(date.getTime())) {
                        return formatDateObject(date);
                    }
                } catch (e) {
                    // Date parsing failed
                }
                
                return null;
            }
            
            function showToast(message, type) {
                // Create toast container if it doesn't exist
                if ($('#toast-container').length === 0) {
                    $('body').append('<div id="toast-container" style="position: fixed; top: 20px; right: 20px; z-index: 99999;"></div>');
                }
                
                var bgColor = type === 'error' ? '#dc3545' : '#28a745';
                var icon = type === 'error' ? '⚠️' : '✓';
                
                var toast = $('<div class="custom-toast" style="background-color: ' + bgColor + '; color: white; padding: 15px 20px; margin-bottom: 10px; border-radius: 5px; box-shadow: 0 4px 6px rgba(0,0,0,0.1); min-width: 300px; max-width: 500px; animation: slideIn 0.3s ease-out;">' +
                    '<div style="display: flex; align-items: center; gap: 10px;">' +
                    '<span style="font-size: 20px; font-weight: bold;">' + icon + '</span>' +
                    '<span>' + message + '</span>' +
                    '</div>' +
                    '</div>');
                
                $('#toast-container').append(toast);
                
                setTimeout(function() {
                    toast.fadeOut(300, function() {
                        $(this).remove();
                    });
                }, 6000);
            }
        });
        
        // Add toast animation styles if not already added
        if ($('head').find('style[data-toast-animation]').length === 0) {
            $('head').append('<style data-toast-animation>@@keyframes slideIn { from { transform: translateX(400px); opacity: 0; } to { transform: translateX(0); opacity: 1; } }</style>');
        }
    </script>
}
