    <div class="hotel-dt-gallery-section mb-50">
        <div class="container">
            <div class="room-img-group">
                <div class="row g-2">
                    <div class="col-lg-6">
                        <div class="gallery-img-wrap">
                            <div class="swiper hotel-dt-gallery-slider">
                                <div class="swiper-wrapper">
                                    @if (hotel?.HotelImages != null && hotel.HotelImages.Any())
                                    {
                                        @foreach (var image in hotel.HotelImages.Take(5))
                                        {
                                            <div class="swiper-slide">
                                                <img src="@image" alt="@hotel.BusinessName" onerror="this.src='/img/innerpages/hotel-dt-gallery-img1.jpg'">
                                            </div>
                                        }
                                    }
                                    else if (!string.IsNullOrEmpty(hotel?.ImageUrl))
                                    {
                                        <div class="swiper-slide">
                                            <img src="@hotel.ImageUrl" alt="@hotel.BusinessName" onerror="this.src='/img/innerpages/hotel-dt-gallery-img1.jpg'">
                                        </div>
                                    }
                                    else
                                    {
                                        <div class="swiper-slide">
                                            <img src="/img/innerpages/hotel-dt-gallery-img1.jpg" alt="">
                                        </div>
                                        <div class="swiper-slide">
                                            <img src="/img/innerpages/hotel-dt-gallery-img6.jpg" alt="">
                                        </div>
                                        <div class="swiper-slide">
                                            <img src="/img/innerpages/hotel-dt-gallery-img7.jpg" alt="">
                                        </div>
                                    }
                                </div>
                                <div class="slider-btn-grp">
                                    <div class="slider-btn hotel-dt-gallery-slider-prev">
                                        <svg width="12" height="12" viewBox="0 0 12 12"
                                            xmlns="http://www.w3.org/2000/svg">
                                            <path
                                                d="M4.84554 6.00254L9.33471 1.51317C9.45832 1.38985 9.52632 1.22498 9.52632 1.04917C9.52632 0.873268 9.45832 0.708488 9.33471 0.584976L8.94135 0.191805C8.81793 0.0680975 8.65295 0 8.47715 0C8.30134 0 8.13656 0.0680975 8.01305 0.191805L2.66798 5.53678C2.54398 5.66068 2.47608 5.82624 2.47657 6.00224C2.47608 6.17902 2.54388 6.34439 2.66798 6.46839L8.00808 11.8082C8.13159 11.9319 8.29637 12 8.47227 12C8.64808 12 8.81286 11.9319 8.93647 11.8082L9.32973 11.415C9.58564 11.1591 9.58564 10.7425 9.32973 10.4867L4.84554 6.00254Z" />
                                        </svg>
                                    </div>
                                    <div class="slider-btn hotel-dt-gallery-slider-next">
                                        <svg width="12" height="12" viewBox="0 0 12 12"
                                            xmlns="http://www.w3.org/2000/svg">
                                            <path
                                                d="M7.15446 6.00254L2.66529 1.51317C2.54168 1.38985 2.47368 1.22498 2.47368 1.04917C2.47368 0.873268 2.54168 0.708488 2.66529 0.584976L3.05865 0.191805C3.18207 0.0680975 3.34705 0 3.52285 0C3.69866 0 3.86344 0.0680975 3.98695 0.191805L9.33202 5.53678C9.45602 5.66068 9.52392 5.82624 9.52343 6.00224C9.52392 6.17902 9.45612 6.34439 9.33202 6.46839L3.99192 11.8082C3.86841 11.9319 3.70363 12 3.52773 12C3.35192 12 3.18714 11.9319 3.06353 11.8082L2.67027 11.415C2.41436 11.1591 2.41436 10.7425 2.67027 10.4867L7.15446 6.00254Z" />
                                        </svg>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class="row g-2">
                            @{
                                var additionalImages = new List<string>();
                                
                                // Add room type images
                                if (Model.RoomTypes != null && Model.RoomTypes.Any())
                                {
                                    foreach (var roomType in Model.RoomTypes.Take(2))
                                    {
                                        if (!string.IsNullOrEmpty(roomType.PicturePath))
                                        {
                                            additionalImages.Add(roomType.PicturePath);
                                        }
                                    }
                                }
                                
                                // Add hotel images if not enough
                                if (hotel?.HotelImages != null && additionalImages.Count < 2)
                                {
                                    foreach (var img in hotel.HotelImages.Skip(1).Take(2 - additionalImages.Count))
                                    {
                                        if (!additionalImages.Contains(img))
                                        {
                                            additionalImages.Add(img);
                                        }
                                    }
                                }
                                
                                // Fallback to defaults if still not enough
                                if (additionalImages.Count == 0)
                                {
                                    additionalImages.Add("/img/innerpages/hotel-dt-gallery-img2.jpg");
                                    additionalImages.Add("/img/innerpages/hotel-dt-gallery-img3.jpg");
                                }
                                else if (additionalImages.Count == 1)
                                {
                                    additionalImages.Add("/img/innerpages/hotel-dt-gallery-img3.jpg");
                                }
                            }
                            
                            @for (int i = 0; i < 2; i++)
                            {
                                <div class="col-6">
                                    <div class="gallery-img-wrap">
                                        <a data-fancybox="gallery-01" href="@additionalImages[i]">
                                            <img src="@additionalImages[i]" alt="@hotel?.BusinessName" onerror="this.src='/img/innerpages/hotel-dt-gallery-img@(i+2).jpg'">
                                        </a>
                                    </div>
                                </div>
                            }

                            <div class="col-6">
                                <div class="gallery-img-wrap two active">
                                    @if (!string.IsNullOrEmpty(hotel?.VideoPath))
                                    {
                                        var videoThumbImg = additionalImages.Count > 2 ? additionalImages[2] : "/img/innerpages/hotel-dt-gallery-img4.jpg";
                                        <img src="@videoThumbImg" alt="@hotel?.BusinessName" onerror="this.src='/img/innerpages/hotel-dt-gallery-img4.jpg'">
                                        <a data-fancybox="gallery-01" href="@hotel!.VideoPath"><i class="bi bi-play-circle"></i> Watch Video</a>
                                    }
                                    else
                                    {
                                        var videoThumbImage = additionalImages.Count > 2 ? additionalImages[2] : 
                                                             (hotel?.HotelImages != null && hotel.HotelImages.Count > 2 ? hotel.HotelImages[2] : "/img/innerpages/hotel-dt-gallery-img4.jpg");
                                        <img src="@videoThumbImage" alt="@hotel?.BusinessName" onerror="this.src='/img/innerpages/hotel-dt-gallery-img4.jpg'">
                                        <a data-fancybox="gallery-01" href="@videoThumbImage"><i class="bi bi-images"></i> View Image</a>
                                    }
                                </div>
                            </div>
                            <div class="col-6">
                                <div class="gallery-img-wrap two active">
                                    @{
                                        var moreImagesThumb = hotel?.HotelImages != null && hotel.HotelImages.Count > 3 ? hotel.HotelImages[3] : "/img/innerpages/hotel-dt-gallery-img5.jpg";
                                    }
                                    <img src="@moreImagesThumb" alt="@hotel?.BusinessName" onerror="this.src='/img/innerpages/hotel-dt-gallery-img5.jpg'">
                                    <button class="StartSlideShowFirstImage"><i class="bi bi-plus-lg"></i> View More
                                        Images</button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="others-image-wrap d-none">
        @if (hotel?.HotelImages != null && hotel.HotelImages.Any())
        {
            @foreach (var image in hotel.HotelImages)
            {
                <a href="@image" data-fancybox="images"></a>
            }
        }
        @if (Model.RoomTypes != null)
        {
            @foreach (var roomType in Model.RoomTypes)
            {
                @if (!string.IsNullOrEmpty(roomType.PicturePath))
                {
                    <a href="@roomType.PicturePath" data-fancybox="images"></a>
                }
            }
        }
        @if (hotel?.HotelImages == null || !hotel.HotelImages.Any())
        {
            <a href="/img/innerpages/hotel-dt-gallery-img1.jpg" data-fancybox="images"></a>
            <a href="/img/innerpages/hotel-dt-gallery-img2.jpg" data-fancybox="images"></a>
            <a href="/img/innerpages/hotel-dt-gallery-img3.jpg" data-fancybox="images"></a>
            <a href="/img/innerpages/hotel-dt-gallery-img4.jpg" data-fancybox="images"></a>
            <a href="/img/innerpages/hotel-dt-gallery-img5.jpg" data-fancybox="images"></a>
        }
    </div>

